@model Demo.Models.EntityFramework.KeyViewModel

@{
    ViewData["Title"] = "Identity column";
}

<h2>Identity column</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="IdentityColumn">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="KeySuggestionForNoAutoGeneratedKey" class="control-label"></label>
                <input asp-for="KeySuggestionForNoAutoGeneratedKey" class="form-control" />
                <span asp-validation-for="KeySuggestionForNoAutoGeneratedKey" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create entities" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" asp-controller="Home">Back to home</a>
</div>

